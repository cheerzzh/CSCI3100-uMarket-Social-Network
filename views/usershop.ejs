<!doctype html>
<html>
  <head>
    <title>uMarket</title>
    
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <link rel="stylesheet" href="stylesheets/include/paper.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="stylesheets/timeline.css">
    <link rel="stylesheet" href="stylesheets/search.css">

  </head>
  <body style=" padding-top: 100px; ">
    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          </button>
          <a id="logo" class="navbar-brand" href="/">uMarket</a>
        </div>
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
          <ul class="nav navbar-nav navbar-right">
            <li class="dropdown">
              <a id="notification" href="#" class="dropdown-toggle navbar-icons" data-toggle="dropdown">
                <i class="fa fa-bell-o"></i>
              </a>
              <ul id="notifications" class="dropdown-menu" role="menu">
                <script id="notifications-template" type="text/x-handlebars-template">
                <li class="dropdown-title">
                  <h5><b>Notifications</b></h5>
                </li>
                {{#each notifications}}
                <li>
                  <a href="#">
                    <h5><b>{{title}}</b></h5>
                    {{{post}}}
                  </a>
                </li>
                {{/each}}
                <li>
                  <a href="#" class="text-center">
                    <span><b><i class="fa fa-bars"></i> More</b></span>
                  </a>
                </li>
                </script>
              </ul>
            </li>
            <li class="dropdown">
              <a id="inbox" href="#" class="dropdown-toggle navbar-icons" data-toggle="dropdown">
                <i class="fa fa-inbox"></i>
              </a>
              <ul id="messages" class="dropdown-menu" role="menu">
                <script id="messages-template" type="text/x-handlebars-template">
                <li class="dropdown-title">
                  <h5><b>Messages</b></h5>
                </li>
                {{#each messages}}
                <li>
                  <a href="#">
                    <h6><b>{{user}}</b></h6>
                    {{{message}}}
                  </a>
                </li>
                {{/each}}
                <li>
                  <a href="#" class="text-center">
                    <span><b><i class="fa fa-bars">bar</i> More</b></span>
                  </a>
                </li>
                </script>
              </ul>
            </li>
            
            <li><a href="/uploadItem" class="navbar-icons"><i class="fa fa-upload"></i></a></li>
            <li><a href="/wishList" class="navbar-icons"><i class="fa fa-heart"></i></a></li>
            <li><a href="#" class="navbar-icons"><i class="fa fa-cog"></i></a></li>
            <li><a href="/logout" class="navbar-icons"><i class="fa fa-sign-out"></i></a></li>
            
            <!-- name and pic get from ajax call-->
            <li>
              <a href="/profile" id='username-nav'><img class="navbar-avatar" src="#" alt="Profile" id="avatar-nav"> </a>
            </li>
            
            
          </ul>
          <form class="navbar-form navbar-right" role="search" action='/search' method='get'>
            <div class="form-group">
              <div class="input-group">
                <input type="text" class="form-control" placeholder="Search" name="searchKey" >
                <div class="input-group-addon"><i class="fa fa-search"></i></div>
              </div>
            </div>
          </form>
        </div>
      </div>
    </nav>
    
  <!--
  </div> -->
  <!-- Javascript -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.0.0-beta1/jquery.min.js"></script>
  <script type="text/javascript" src="javascripts/include/paper.min.js"></script>
  <script type="text/javascript" src="javascripts/include/handlebars-v2.0.0.js"></script>
  <script type="text/javascript" src="javascripts/include/jquery.autosize.min.js"></script>
  <script src="assets/js/jquery.backstretch.min.js"></script>
  <script type='text/javascript'>
  window.targetUser =<%-JSON.stringify(targetuser)%> // get item from server
  window.user = <%-JSON.stringify(user)%>
  // console.log(item)
  </script>
  <script src="javascripts/fillUserInfo.js"></script>
  <script src="javascripts/usershop.js"></script>
<div class="container">
<div class="row">
   <div class="col-xs-12 col-sm-12 col-md-12 col-lg-3 col-md-12">
    
          <div class="thumbnail">
            <div class="panel-body">
              <div class="row">
                <div class="col-xs-4 col-sm-4 col-lg-12 text-center">
                  <img src="#" alt="" class="center-block img-circle img-thumbnail img-responsive" id="avatar-profile" >
                </div>
                
                <div class="col-xs-12 col-sm-8 col-lg-12">
                  

                  <center><h2 id = "username-profile"></h2></center>
                  <h3 id = "email-profile"></h3>
                  <h3 id = "university-profile"></h3>
                    
                  
                
                  <p id = "description-profile"></p>
                
                </div>
                <div class="clearfix"></div>
                <div class="col-xs-12 col-sm-4 col-lg-4">
                  <h2 id = "followerNumber"><strong></strong></h2>
                  <p><small>Followers</small></p>
                </div>
                
                <div class="col-xs-12 col-sm-4 col-lg-4">
                  <h2 id = "followingNumber"><strong></strong></h2>
                  <p><small>Following</small></p>
                </div>
                
                <div class="col-xs-12 col-sm-4 col-lg-4">
                  <h2 id = "itemNumber"><strong></strong></h2>
                  <p><small>Posted items</small></p>
                </div>
                
                <div class="col-xs-12 col-sm-12 col-lg-12">
                  <button class="btn btn-success btn-md followButton  btn-block" >
                        Follow
                  </button>
                </div>
                <div class="col-xs-12 col-sm-12 col-lg-12">
                  <button class="btn btn-warning btn-md unfollowButton  btn-block" >
                        Unfollow
                  </button>
                </div>
                <br>
                <div class="col-xs-12 col-sm-12 col-lg-12">
                <input type="text" placeholder="Want to Chat?" class="form-control input-lg widget-profile-input">
                </div>
              </div>
            </div>
          </div>
          

          
   </div>
   
   <div class="col-lg-9 col-md-12">
    
            <div class="panel-heading">
     <div class="collapse navbar-collapse" id="myNavbar">
              <ul id="myTab" class="nav nav-tabs">
                <li class="active"><a href="#selling" data-toggle="tab">
                  <b>In progress</b>
                </a></li>
                <li><a href="#reserved" data-toggle="tab"><b>Reserved</b></a></li>
                <li><a href="#traded" data-toggle="tab"><b>Traded</b></a></li>
                <li><a href="#withdrawed" data-toggle="tab"><b>Withdrawed</b></a></li>
              </ul>
      </div>
      </div>
      </div>
   
   <div class="col-lg-9 col-md-12">
     <div id="myTabContent" class="tab-content">
   <div class="tab-pane fade in active" id="selling">
   <section class="col-xs-12 col-sm-12 col-md-12" id="item">
   <script id="item-template" type="text/x-handlebars-template">
   {{#each itemEntry}}
      <div class="col-lg-4 col-md-12">
      <div class="thumbnail">
         <a href={{itemLink}}><img src={{itemImageLink}} alt="can not found" style="height : 150px"></a>
         <div class="caption">
            <a href={{itemLink}}><h3>{{itemname}}</h3></a>
                <div class="row">
                <div class="col-lg-6 ol-md-6 col-xs-12">
                    <p>Status:{{itemStatus}}</p>
                </div>
                <div class="col-lg-6 col-md-6 clo-xs-12">
                    <p>Price:{{itemPrice}}</p>
                </div>
            </div>
            <div class="row">
              <div class="col-lg-4 col-md-6">
                <i class="glyphicon glyphicon-time"></i> <span>{{updateTime}}</span>
              </div>
              
              
              <div class="col-lg-8 col-md-6">
              
               <div class="actions">
                <div class="actions-content">
                <ul style="list-style: none;">
                
                  <span class="heartButton" id = {{heartID}} value={{itemID}}><i class="fa fa-heart" style="{{heartStyle}};font-size:120%;" ></i>
                  <font color="grey" >{{wishedCount}}</font></span>
                  <li class={{buyID}}><span class="buyButton" id = {{buyID}} value={{itemID}}><i class="fa fa-shopping-cart" style="{{buyStyle}};font-size:120%;" ></i>
                  <font color="grey" >{{buyDesciption}}</font></span></li>
                
                </ul>
                </div>
              </div>
              </div>
              
              <div class="col-lg-12 col-md-12">
                <i class="glyphicon glyphicon-calendar"></i> <span>{{updateDate}}</span>
              </div>
              
            </div>
              
         </div>
      </div>
   </div>
   {{/each}}
   </script>
   </section>
   </div>

   
      
