    <div id="all">

        <div id="content">
            <div class="container">
                 <div class="col-md-3">
                 </div>
                <div class="col-md-9">

                    <div class="row" id="productMain">
                        <div class="col-sm-6">
                            <div id="mainImage">
                                <img src="http://placehold.it/800x800" alt="" class="img-responsive">
                            </div>

                            <div class="ribbon sale">
                                <div class="theribbon">In Progress</div>
                                <div class="ribbon-background"></div>
                            </div>
                            <!-- /.ribbon -

                            <div class="ribbon new">
                                <div class="theribbon">NEW</div>
                                <div class="ribbon-background"></div>
                            </div>
                            -->

                        </div>
                        <div class="col-sm-6">
                            <div class="box">
                                <h1 class="text-center" id="item-name"></h1>
                                <p class="goToDescription"><a href="#details" class="scroll-to">Scroll to product details, material & care and sizing</a>
                                </p>
                                <p class="price" id = "item-price"></p>

                                <p class="text-center buttons">
                                    <a href="#" class="btn btn-primary" id="wantToBuy"><i class="fa fa-shopping-cart"></i> Click to Buy</a> 
                                    <a href="#" class="btn btn-default" id="addToWishlist"><i class="fa fa-heart"></i> Add to wishlist</a>
                                </p>


                            </div>

                            <div class="row" id="thumbs">
                                <div class="col-xs-4">
                                    <a href="#" class="thumb">
                                        <img src="http://placehold.it/800x400" alt="" class="img-responsive">
                                    </a>
                                </div>
                                <div class="col-xs-4">
                                    <a href="#" class="thumb">
                                        <img src="http://placehold.it/800x400" alt="" class="img-responsive">
                                    </a>
                                </div>
                                <div class="col-xs-4">
                                    <a href="#" class="thumb">
                                        <img src="http://placehold.it/800x400" alt="" class="img-responsive">
                                    </a>
                                </div>
                            </div>
                        </div>

                    </div>


                    <div class="box" id="details">
                        <p>
                            <h4>Product details</h4>
                            <p id="item-description"></p>

                            <blockquote>
                                <p><em id="item-description"></em>
                                </p>
                            </blockquote>

                            <hr>

                    </div>

                    <div class="row same-height-row">
                        <div class="col-md-3 col-sm-6">
                            <div class="box same-height">
                                <h3>You may also like these products</h3>
                            </div>
                        </div>

                        <div class="col-md-3 col-sm-6">
                            <div class="product same-height">
                                <div class="flip-container">
                                    <div class="flipper">
                                        <div class="front">
                                            <a href="detail.html">
                                                <img src="http://placehold.it/800x800" alt="" class="img-responsive">
                                            </a>
                                        </div>
                                        <div class="back">
                                            <a href="detail.html">
                                                <img src="http://placehold.it/800x800" alt="" class="img-responsive">
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <a href="detail.html" class="invisible">
                                    <img src="http://placehold.it/800x800" alt="" class="img-responsive">
                                </a>
                                <div class="text">
                                    <h3>Fur coat</h3>
                                    <p class="price">$143</p>
                                </div>
                            </div>
                            <!-- /.product -->
                        </div>

                        <div class="col-md-3 col-sm-6">
                            <div class="product same-height">
                                <div class="flip-container">
                                    <div class="flipper">
