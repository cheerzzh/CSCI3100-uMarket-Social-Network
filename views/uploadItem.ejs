                  <a href="#" class="text-center">
                    <span><b><i class="fa fa-bars"></i> More</b></span>
                  </a>
                </li>
              </script>
            </ul>
          </li>
          <li class="dropdown">
            <a id="inbox" href="#" class="dropdown-toggle navbar-icons" data-toggle="dropdown">
              <i class="fa fa-inbox"></i>
            </a>
            <ul id="messages" class="dropdown-menu" role="menu">
              <script id="messages-template" type="text/x-handlebars-template">
                <li class="dropdown-title">
                  <h5><b>Messages</b></h5>
                </li>
                {{#each messages}}
                <li>
                  <a href="#">
                    <h6><b>{{user}}</b></h6>
                    {{{message}}}
                  </a>
                </li>
                {{/each}}
                <li>
                  <a href="#" class="text-center">
                    <span><b><i class="fa fa-bars">bar</i> More</b></span>
                  </a>
                </li>
              </script>
            </ul>
          </li>
          
          <li><a href="/uploadItem" class="navbar-icons"><i class="fa fa-upload"></i></a></li>
          <li><a href="#" class="navbar-icons"><i class="fa fa-heart"></i></a></li>
          <li><a href="#" class="navbar-icons"><i class="fa fa-cog"></i></a></li>
          <li><a href="/logout" class="navbar-icons"><i class="fa fa-sign-out"></i></a></li>
  
          <!-- name and pic get from ajax call-->
          <li>
            <a href="/profile"><img class="navbar-avatar" src="https://en.gravatar.com/userimage/23763355/e7bbf514106dc2fd9ddb4e8160a72e8c.png" alt="Profile"> sudharti </a>
          </li>

        </ul>
        <form class="navbar-form navbar-right" role="search" action="/search" method="post">
          <div class="form-group">
            <div class="input-group">
              <input type="text" class="form-control" placeholder="Search" name = "searchKey">
              <div class="input-group-addon"><i class="fa fa-search"></i></div>
            </div>
          </div>
        </form>
      </div>
    </div>
  </nav>

  <!--NAV BAR END-->
<div class="container" style=" padding-top: 100px; ">
<div class="row">

	      <div class="col-lg-3 col-md-3">
        <div class="card">
          <div class="cover" data-fade="1500" data-duration="2500">
            <ul class="cover-pics">
              <li><img src="images/img-1.jpg"></li>
              <li><img src="images/img-2.jpg"></li>
              <li><img src="images/img-3.jpg"></li>
              <li><img src="images/img-4.jpg"></li>
            </ul>
            <img src="https://en.gravatar.com/userimage/23763355/e7bbf514106dc2fd9ddb4e8160a72e8c.png" alt="" class="avatar">
          </div>
          <div class="about">
            <h3 class="name">Sudharsanan M</h3>
            <h5 class="username">@sudharti</h5>
            <span class="content">22 | Hacker | Technophile | Blogger | Android Dev | Rails Fanboy | Rubyist</span>
            <label id="location"><i class="fa fa-map-marker"></i> Dallas, Texas</label>
          </div>
        </div><br>
        <div class="panel panel-default">
          <div class="panel-heading">
            <h3 class="panel-title"><b>Item List</b></h3>
          </div>
          <div class="panel-body">
          <ul id="userItemList"></ul>
          <!--
            <ul id="trends" class="panel-list">
              <script id="trends-template" type="text/x-handlebars-template">
                {{#each trends}}
                <li><a href="#">{{trend}}</a></li>
                {{/each}}
              </script>
            </ul>
            -->
          </div>
        </div>
      </div>


	<div class="col-lg-9 col-md-9">
	<div class="panel panel-default">
	  <div id="new-micropost" class="panel-body">
		<div id='results'></div>
		<div id='userInfo'></div>



	  <h2>I Want to Sell</h2>
	  <form role="form" action="/postItem" method="post" enctype="multipart/form-data">
	    <div class="form-group ">
	      <label for="email">Item name * :</label>
	      <input type="text" class="form-control" name="itemName" placeholder="Enter item name" required>
	    </div>
	    <div class="form-group">
		  <label for="comment">Description * :</label>
		  <textarea class="form-control" rows="5" name="description" placeholder="Say something about your lovely items"></textarea>
		</div> 
		
		<div class="form-group ">
	      <label for="email">Price (HKD) * :</label>
	      <input type="number" class="form-control" name="price" placeholder="" min="0" required>
	    </div>
		
		<div class="form-group ">
		<label for="comment">Condition (%):</label>
		<input type="range" min="0" max="100" value="100" step="5" name="condition" onchange="showValue(this.value)" /><span id="range">100</span>
		</div>

		<div class="form-group ">
	      <label for="email">Reference link:</label>
	      <input type="url" class="form-control" name="refLink" placeholder="May be from amazon ?">
	    </div>

		<div class="form-group">
	      <label class="control-label">You may upload pictures (<= 5)</label>
		<input id="input-21" type="file"  multiple class="file-loading" name = 'images'>
		</div>
	    <button type="submit" class="btn btn-default" >Post ~</button>
	  </form>

	    </div>
        </div>
        </div>

  <!--show list of item if exists-->
  



</div>
</div>

</body>
</html>
